{
  "name": "@fastconsig/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/exports.js",
  "exports": {
    ".": {
      "types": "./dist/exports.d.ts",
      "import": "./dist/exports.js"
    }
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src/server.ts src/exports.ts --format esm --dts",
    "start": "node dist/server.js",
    "types": "dist/exports.d.ts",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fastconsig/database": "workspace:*",
    "@fastconsig/shared": "workspace:*",
    "@fastify/cookie": "^11.0.0",
    "@fastify/cors": "^10.0.0",
    "@fastify/helmet": "^13.0.0",
    "@fastify/jwt": "^9.0.0",
    "@fastify/multipart": "^9.0.0",
    "@fastify/rate-limit": "^10.0.0",
    "@trpc/server": "^11.0.0-rc.502",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.12.0",
    "dayjs": "^1.11.12",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "fastify": "^5.0.0",
    "nodemailer": "^6.9.14",
    "pdfkit": "^0.15.0",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "redis": "^4.7.0",
    "superjson": "^2.2.1",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@fastconsig/config": "workspace:*",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.1.0",
    "@types/nodemailer": "^6.4.15",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^9.9.0",
    "tsup": "^8.2.4",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}
